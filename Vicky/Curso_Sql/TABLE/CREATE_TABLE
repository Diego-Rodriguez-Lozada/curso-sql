CREATE TABLE person(
id int,
Name varchar(45),
Age int, 
email varchar (100),
created date
);

CREATE TABLE Person2(
id int NOT NULL,
Name varchar(45) NOT NULL,
Age int, 
email varchar (100),
created date
);

CREATE TABLE Person3(
id int NOT NULL, 
Name varchar(45) NOT NULL,
Age int, 
email varchar (100),
created datetime,
UNIQUE (id)
);

CREATE TABLE Person4(
id int NOT NULL, 
Name varchar(45) NOT NULL,
Age int, 
email varchar (100),
created datetime,
PRIMARY KEY (id)
);

CREATE TABLE Person5(
id int NOT NULL, 
Name varchar(45) NOT NULL,
Age int, 
email varchar (100),
created datetime,
UNIQUE (id),
PRIMARY KEY (id),
CHECK (Age<18)
);

CREATE TABLE Person6(
id int NOT NULL, 
Name varchar(45) NOT NULL,
Age int, 
email varchar (100),
created datetime DEFAULT CURRENT_TIMESTAMP(),
UNIQUE (id),
PRIMARY KEY (id),
CHECK (Age<18)
);

CREATE TABLE Person7(
id int NOT NULL AUTO_INCREMENT, 
Name varchar(45) NOT NULL,
Age int, 
email varchar (100),
created datetime DEFAULT CURRENT_TIMESTAMP(),
UNIQUE (id),
PRIMARY KEY (id),
CHECK (Age<18)
);



